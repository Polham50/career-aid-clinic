{
  "name": "careeraid-clinic",
  "version": "1.0.0",
  "description": "An AI-powered career guidance platform.",
  "scripts": {
    "start": "esbuild index.tsx --bundle --outfile=dist/bundle.js --servedir=.",
    "start:backend": "nodemon --exec ts-node server.ts",
